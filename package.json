    "url": "https://github.com/christophercatt/licensed-ci-rebuild/issues"
  },
  "homepage": "https://github.com/christophercatt/licensed-ci-rebuild#readme",
  "dependencies": {
    "@actions/core": "^1.2.5"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.24.0",
    "eslint": "^7.4.0",
    "jest": "^26.1.0"
  }
}
